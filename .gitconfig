[user]
  name = Adalberto Junior
  email = adalrsjr1@gmail.com
[color]
  ui = true
[format]
  pretty = oneline
[init]
  defaultBranch = "master"
[alias]
  ignore = "!gi() { curl -L -s https://www.gitignore.io/api/$@ ;}; gi"
  graph = "log --oneline --graph --decorate --all"
  commit = "commit -S"
[diff]
  tool = nvimdiff
[difftool]
  prompt = false
[merge]
  tool = nvimdiff
[mergetool]
  prompt = false
[credential]
  helper = store
